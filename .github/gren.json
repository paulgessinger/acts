{
  "dataSource": "prs",
  "milestoneMatch": "{{tag_name}}",
  "ignoreIssuesWith": [
    "wontfix"
  ],
  "groupBy": {
    "Improvement:": ["Improvement"],
    "New Feature:": ["New Feature"],
    "Infrastructure": ["Infrastructure"],
    "Bug Fixes:": ["Bug"]
  },
  "template": {
    "issue": "- {{name}} [{{text}}]({{url}})"
  }
}
